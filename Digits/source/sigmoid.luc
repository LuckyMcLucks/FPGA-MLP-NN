module sigmoid (

    input x[32],
    output out[32]
) {
    sig y[32]
    const M = 4
    const C = 50
    always {
        
        
        if (x[31]==0){
            y = x/M+ C
            
        }else{
            y= -(-x/M)+C
            
            
        }
        if (x[31] ==1){
          if(-x >=d150){
            y = 5
        }  
        }else{
        if(x >=d150){
            y = 95
        }
        }
        
         
        
        out = y
        
        
    }
}